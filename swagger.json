{
  "swagger": "2.0",
  "info": {
    "title": "European Premier League API",
    "description": "Get information about the European Premier League, 2022 Season",
    "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
    "http"
  ],
  "paths": {
    "/teams/": {
      "get": {
        "tags": [
          "teams"
        ],
        "description": "Retrieve information of all teams",
        "responses": {
          "200": {
            "description": "List of all teams"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "post": {
        "tags": [
          "teams"
        ],
        "description": "Create a new team",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "Team ID": { "type": "string", "example": "T000" },
                "Team Name": { "type": "string", "example": "any" },
                "Location": { "type": "string", "example": "any" },
                "Founded Year": { "type": "integer", "example": 2023 },
                "Coach ID": { "type": "string", "example": "C0000" }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Team created successfully"
          },
          "400": {
            "description": "Bad Request"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/teams/{TeamID}": {
      "get": {
        "tags": [
          "teams"
        ],
        "description": "Retrieve information of a specific team by Team ID",
        "parameters": [
          {
            "name": "TeamID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Team data retrieved successfully"
          },
          "404": {
            "description": "Team not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "put": {
        "tags": [
          "teams"
        ],
        "description": "Update a specific team's information by Team ID",
        "parameters": [
          {
            "name": "TeamID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "Team Name": { "type": "string", "example": "any" },
                "Location": { "type": "string", "example": "any" },
                "Founded Year": { "type": "integer", "example": 2023 },
                "Coach ID": { "type": "string", "example": "C0001" }
               
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Team updated successfully"
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "Team not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
     
      "delete": {
        "tags": [
          "teams"
        ],
        "description": "Delete a specific team by Team ID",
        "parameters": [
          {
            "name": "TeamID",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "204": {
            "description": "Team deleted successfully"
          },
          "400": {
            "description": "Bad Request"
          },
          "404": {
            "description": "Team not found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/players/": {
      "get": {
        "tags": [
          "players"
        ],
        "description": "Retrieve information of all players",
